{
  "version": 2,
  "builds": [
    {
      "src": "api/validateMarket.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/resolveMarket.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/validateMarket",
      "dest": "/api/validateMarket.js",
      "methods": [
        "GET",
        "POST",
        "OPTIONS"
      ]
    },
    {
      "src": "/api/resolveMarket",
      "dest": "/api/resolveMarket.js",
      "methods": [
        "POST",
        "OPTIONS"
      ]
    },
    {
      "src": "/",
      "dest": "/api/validateMarket.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "echo 'No build needed for serverless functions'",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./"
}