{
  "version": 2,
  "builds": [
    { 
      "src": "api/validateMarket.js", 
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    { 
      "src": "/api/validate-market", 
      "dest": "/api/validateMarket.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    { 
      "src": "/api/health", 
      "dest": "/api/validateMarket.js" 
    },
    { 
      "src": "/api/test-cors", 
      "dest": "/api/validateMarket.js" 
    }
  ]
}